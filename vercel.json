{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "out"
        }
      }
    ],
    "build": {
      "env": {
        "DATABASE_URL": "@database_url"
      },
      "commands": {
        "postbuild": "npx prisma generate"
      }
    }
  }
  